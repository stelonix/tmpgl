{
	"fonts":
	{
		"exts": [".ttf", ".otf"],
		"type": "FNT"
	},
	"maps":
	{
		"exts": [".json"],
		"type": "MAP"
	},
	"scripts":
	{
		"exts": [".lua"],
		"type": "LUA"
	},
	"shaders":
	{
		"exts": [".glsl", ".vert", ".frag"],
		"type": "SHD"
	},
	"sprites":
	{
		"exts": [".json"],
		"type": "SPR",
		"/img": {
			"exts": [".png"],
			"type": "TEX",
			"lazy": false
		}
	},
	"textures":
	{
		"exts": [".png"],
		"lazy": true,
		"type": "TEX"
	},
	"tiles":
	{
		"exts": [".json"],
		"type": "TIL",
		"/img": {
			"exts": [".png"],
			"type": "TEX",
			"lazy": false
		}
	}
}
